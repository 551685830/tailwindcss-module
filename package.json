{
  "name": "@nuxtjs/tailwindcss",
  "version": "4.2.1",
  "description": "TailwindCSS module for Nuxt",
  "repository": "nuxt-community/tailwindcss-module",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "nuxi dev playground",
    "docs": "nuxt docs",
    "lint": "eslint --ext .js,.ts,.vue",
    "prepack": "yarn build",
    "release": "yarn test && standard-version && git push --follow-tags && npm publish",
    "test": "yarn lint && mocha -r jiti/register test/*.test.*"
  },
  "dependencies": {
    "@nuxt/postcss8": "^1.1.3",
    "autoprefixer": "^10.4.0",
    "chalk": "^4.1.1",
    "clear-module": "^4.1.1",
    "consola": "^2.15.3",
    "defu": "^5.0.0",
    "postcss": "^8.3.11",
    "postcss-custom-properties": "^12.0.0",
    "postcss-nesting": "^10.0.1",
    "tailwind-config-viewer": "^1.6.2",
    "tailwindcss": "^2.2.19",
    "ufo": "^0.7.9"
  },
  "devDependencies": {
    "@nuxt/bridge": "latest",
    "@nuxt/kit": "npm:@nuxt/kit-edge",
    "@nuxtjs/eslint-config-typescript": "^7.0.2",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "chai": "^4.3.4",
    "codecov": "^3.8.3",
    "eslint": "^8.2.0",
    "listhen": "^0.2.5",
    "mocha": "^9.1.3",
    "nuxt-edge": "latest",
    "ohmyfetch": "^0.4.6",
    "standard-version": "^9.3.2",
    "unbuild": "^0.5.11"
  }
}
